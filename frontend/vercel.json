{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true,
      "develop": true
    }
  },
  "github": {
    "silent": false,
    "autoJobCancelation": true
  },
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}
